package practice_start;
import java.util.Scanner;

public class recursion_first_code {
	public static void reverse(int n) {
	  if(n==100) {
		  System.out.print(" " + n);
		  return;
	  }
	  System.out.print(" "+ n);
	  reverse(n+1);
	}
   public static void main(String[]args) {
	   System.out.println("Plsese enter your number");
	   Scanner in = new Scanner(System.in);
	   int n = in.nextInt();
	   reverse(n);
   }
   
}
